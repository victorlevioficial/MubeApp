{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "database": {
    "rules": "firebase-rtdb-rules.json"
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "mube-63a93",
          "appId": "1:798301748829:android:25d2fd20932bb4bc47d7b4",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "mube-63a93",
          "configurations": {
            "android": "1:798301748829:android:25d2fd20932bb4bc47d7b4",
            "ios": "1:798301748829:ios:83fd689a9de5f7eb47d7b4",
            "web": "1:798301748829:web:f1b96526dcd3f99b47d7b4",
            "windows": "1:798301748829:web:6a67c92e5d83b15347d7b4"
          }
        }
      }
    }
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "disallowLegacyRuntimeConfig": true,
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run lint",
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    }
  ],
  "hosting": {
    "public": "landing_page",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/politicadeprivacidade",
        "destination": "/politicadeprivacidade/index.html"
      },
      {
        "source": "/termosdeuso",
        "destination": "/termosdeuso/index.html"
      },
      {
        "source": "/admin",
        "destination": "/admin/index.html"
      },
      {
        "source": "/admin/**",
        "destination": "/admin/index.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          }
        ]
      }
    ]
  }
}